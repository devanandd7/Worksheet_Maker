{
    "execMap": {
        "js": "node --import ./loadEnv.js"
    },
    "watch": [
        "*.js",
        "routes",
        "models",
        "services",
        "middleware",
        "config"
    ],
    "ext": "js,json",
    "ignore": [
        "node_modules",
        "*.test.js"
    ],
    "env": {
        "NODE_ENV": "development"
    }
}